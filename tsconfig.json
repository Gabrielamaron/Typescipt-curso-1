{
  "compilerOptions": {
    // gravacao de arquivos resultantes
    "outDir": "dist/js",
    // ecmascript que o ts será convertido
    "target": "ES6",
    // não compilar arquivos com erro
    "noEmitOnError": true,
    // não compliar arquivos com implicidade
    "noImplicitAny": true
  },
  "include": ["app/**/*"]
}
